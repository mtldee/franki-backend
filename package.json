{
  "name": "franki-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
"scripts": {
  "start": "NODE_ENV=production node src/index.js",
  "dev": "NODE_ENV=development node src/index.js",

  "dev:drop": "NODE_ENV=development npx sequelize-cli db:drop && npm run dev:create",
  "dev:create": "NODE_ENV=development npx sequelize-cli db:create",
  "dev:migrate": "NODE_ENV=development npx sequelize-cli db:migrate",
  "dev:seed": "NODE_ENV=development npx sequelize-cli db:seed:all",
  "dev:reset": "NODE_ENV=development npx sequelize-cli db:migrate:undo:all && npm run dev:migrate && npm run dev:seed",

  "test:migrate": "NODE_ENV=test npx sequelize-cli db:migrate",
  "test:seed": "NODE_ENV=test npx sequelize-cli db:seed:all",
  "test:reset": "NODE_ENV=test npx sequelize-cli db:migrate:undo:all && npm run test:migrate && npm run test:seed",

  "migrate": "NODE_ENV=production npx sequelize-cli db:migrate",
  "seed": "NODE_ENV=production npx sequelize-cli db:seed:all",
  "start:seed:migrate": "npm run migrate && npm run seed && npm run start"
},
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.15.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.3"
  }
}
